    "tokenColors": [
        // Core structural elements - strong face (bold emphasis)
        {
            "scope": [
                "entity.name.function",
                "entity.name.class",
                "entity.name.type",
                "entity.name.namespace",
                "entity.name.tag",
                "meta.function-call.generic.python",
                "support.function",
                "keyword.control",
                "keyword.operator.new",
                "storage.type.function",
                "storage.type.class",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "{{--vscodet-face-strong}}",
                "fontStyle": "bold"
            }
        },

        // Important constructs - salient face (accent color)
        {
            "scope": [
                "entity.name.type.class",
                "entity.name.type.interface",
                "entity.name.type.enum",
                "support.type",
                "support.class",
                "variable.language.this",
                "variable.language.super",
                "constant.language",
                "meta.import",
                "meta.export"
            ],
            "settings": {
                "foreground": "{{--vscodet-face-salient}}"
            }
        },

        // Attention-grabbing elements - popout face
        {
            "scope": [
                "string",
                "string.quoted",
                "string.template",
                "constant.character.escape",
                "markup.quote",
                "markup.bold",
                "entity.name.tag.yaml"
            ],
            "settings": {
                "foreground": "{{--vscodet-face-popout}}"
            }
        },

        // Critical elements - critical face (errors, warnings, deletions)
        {
            "scope": [
                "invalid",
                "invalid.illegal",
                "invalid.deprecated",
                "markup.deleted",
                "markup.error",
                "diff.removed",
                "diff.deleted",
                "git.deleted",
                "error",
                "diagnostic.error"
            ],
            "settings": {
                "foreground": "{{--vscodet-red-fg}}"
            }
        },

        // Added/inserted elements - use green semantic color
        {
            "scope": [
                "markup.inserted",
                "markup.inserted.diff",
                "diff.inserted",
                "diff.added",
                "git.inserted",
                "git.added",
                "meta.diff.header.to-file",
                "meta.diff.range.added",
                "punctuation.definition.inserted",
                "token.info-token"
            ],
            "settings": {
                "foreground": "{{--vscodet-green-fg}}"
            }
        },

        // Modified elements - use accent/salient color
        {
            "scope": [
                "markup.changed",
                "markup.changed.diff",
                "diff.changed",
                "diff.modified",
                "git.modified",
                "meta.diff.range.changed",
                "punctuation.definition.changed"
            ],
            "settings": {
                "foreground": "{{--vscodet-face-salient}}"
            }
        },

        // Secondary information - faded face (comments, less important)
        {
            "scope": [
                "comment",
                "comment.line",
                "comment.block",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "{{--vscodet-face-faded}}",
                "fontStyle": "italic"
            }
        },

        // Non-comment faded elements (no italic)
        {
            "scope": [
                "markup.quote",
                "meta.preprocessor",
                "entity.other.attribute-name.html",
                "entity.other.attribute-name.class.css",
                "punctuation.separator",
                "punctuation.terminator",
                "punctuation.accessor"
            ],
            "settings": {
                "foreground": "{{--vscodet-face-faded}}"
            }
        },

        // Default elements - default face (variables, identifiers)
        {
            "scope": [
                "variable",
                "variable.other",
                "variable.parameter",
                "variable.object.property",
                "meta.object-literal.key",
                "entity.name.variable",
                "constant.numeric",
                "constant.language.boolean",
                "support.variable"
            ],
            "settings": {
                "foreground": "{{--vscodet-face-default}}"
            }
        },

        // Operators and punctuation - default with bold
        {
            "scope": [
                "keyword.operator",
                "keyword.operator.arithmetic",
                "keyword.operator.comparison",
                "keyword.operator.logical",
                "keyword.operator.assignment",
                "punctuation.definition.string.begin",
                "punctuation.definition.string.end"
            ],
            "settings": {
                "foreground": "{{--vscodet-face-default}}",
                "fontStyle": "bold"
            }
        },

        // Language keywords - salient with bold
        {
            "scope": [
                "keyword",
                "keyword.control.flow",
                "keyword.control.conditional",
                "keyword.control.loop",
                "keyword.control.import",
                "keyword.control.export",
                "storage.type",
                "storage.modifier.async",
                "storage.modifier.static"
            ],
            "settings": {
                "foreground": "{{--vscodet-face-salient}}",
                "fontStyle": "bold"
            }
        },

        // Types and built-ins - salient face
        {
            "scope": [
                "entity.name.type",
                "support.type.primitive",
                "support.type.builtin",
                "support.class.builtin",
                "entity.other.inherited-class"
            ],
            "settings": {
                "foreground": "{{--vscodet-face-salient}}"
            }
        },

        // Decorators and annotations - popout face
        {
            "scope": [
                "entity.name.function.decorator",
                "meta.decorator",
                "punctuation.decorator"
            ],
            "settings": {
                "foreground": "{{--vscodet-face-popout}}"
            }
        },

        // Markup headings - strong face
        {
            "scope": [
                "markup.heading",
                "entity.name.section"
            ],
            "settings": {
                "foreground": "{{--vscodet-face-strong}}",
                "fontStyle": "bold"
            }
        },

        // Links and references - salient face
        {
            "scope": [
                "markup.underline.link",
                "string.other.link"
            ],
            "settings": {
                "foreground": "{{--vscodet-face-salient}}"
            }
        },

        // Code blocks and literal content - default face with subtle background
        {
            "scope": [
                "markup.fenced_code",
                "markup.inline.raw"
            ],
            "settings": {
                "foreground": "{{--vscodet-face-default}}",
                "background": "{{--vscodet-face-subtle}}"
            }
        }
    ],

    "semanticHighlighting": true,

    "semanticTokenColors": {
        // Core structural elements - strong face (bold for importance)
        "namespace": {
            "foreground": "{{--vscodet-face-strong}}",
            "fontStyle": "bold"
        },
        "class": {
            "foreground": "{{--vscodet-face-strong}}",
            "fontStyle": "bold"
        },
        "function": {
            "foreground": "{{--vscodet-face-strong}}",
            "fontStyle": "bold"
        },
        "method": {
            "foreground": "{{--vscodet-face-strong}}"
        },

        // Important type information - salient face (accent color)
        "interface": {
            "foreground": "{{--vscodet-face-salient}}",
            "fontStyle": "bold"
        },
        "struct": {
            "foreground": "{{--vscodet-face-salient}}",
            "fontStyle": "bold"
        },
        "enum": {
            "foreground": "{{--vscodet-face-salient}}",
            "fontStyle": "bold"
        },
        "type": {
            "foreground": "{{--vscodet-face-salient}}"
        },
        "typeParameter": {
            "foreground": "{{--vscodet-face-salient}}"
        },

        // Language constructs - salient with bold
        "keyword": {
            "foreground": "{{--vscodet-face-salient}}",
            "fontStyle": "bold"
        },
        "macro": {
            "foreground": "{{--vscodet-face-salient}}",
            "fontStyle": "bold"
        },

        // Regular variables and data - default face
        "variable": {
            "foreground": "{{--vscodet-face-default}}"
        },
        "parameter": {
            "foreground": "{{--vscodet-face-default}}"
        },
        "property": {
            "foreground": "{{--vscodet-face-default}}"
        },
        "enumMember": {
            "foreground": "{{--vscodet-face-salient}}"
        },

        // Operators - default with bold
        "operator": {
            "foreground": "{{--vscodet-face-default}}",
            "fontStyle": "bold"
        },

        // Literals and content - popout for strings, default for numbers
        "string": {
            "foreground": "{{--vscodet-face-popout}}"
        },
        "number": {
            "foreground": "{{--vscodet-face-default}}"
        },
        "regexp": {
            "foreground": "{{--vscodet-face-popout}}"
        },

        // Meta constructs - appropriate face based on function
        "decorator": {
            "foreground": "{{--vscodet-face-popout}}"
        },
        "event": {
            "foreground": "{{--vscodet-face-salient}}"
        },
        "label": {
            "foreground": "{{--vscodet-face-default}}",
            "fontStyle": "bold"
        },
        "comment": {
            "foreground": "{{--vscodet-face-faded}}",
            "fontStyle": "italic"
        },

        // Behavioral modifiers - emphasize declarations and important states
        "*.declaration": {
            "fontStyle": "bold"
        },
        "*.definition": {
            "fontStyle": "bold"
        },
        "*.readonly": {
            "foreground": "{{--vscodet-face-salient}}"
        },
        "*.deprecated": {
            "foreground": "{{--vscodet-face-faded}}",
            "fontStyle": "strikethrough"
        },
        "*.modification": {
            "fontStyle": "bold"
        },
        "*.documentation": {
            "foreground": "{{--vscodet-face-faded}}"
        },
        "*.defaultLibrary": {
            "foreground": "{{--vscodet-face-salient}}"
        },

        // Specific important combinations
        "class.declaration": {
            "foreground": "{{--vscodet-face-strong}}",
            "fontStyle": "bold"
        },
        "function.declaration": {
            "foreground": "{{--vscodet-face-strong}}",
            "fontStyle": "bold"
        },
        "method.static": {
            "foreground": "{{--vscodet-face-strong}}"
        },
        "function.static": {
            "foreground": "{{--vscodet-face-strong}}",
            "fontStyle": "bold"
        },
        "variable.readonly": {
            "foreground": "{{--vscodet-face-salient}}"
        },
        "property.readonly": {
            "foreground": "{{--vscodet-face-salient}}"
        },
        "parameter.readonly": {
            "foreground": "{{--vscodet-face-salient}}"
        },
        "class.abstract": {
            "foreground": "{{--vscodet-face-salient}}",
            "fontStyle": "bold"
        },
        "method.abstract": {
            "foreground": "{{--vscodet-face-strong}}"
        },
        "function.async": {
            "foreground": "{{--vscodet-face-strong}}",
            "fontStyle": "bold"
        },
        "method.async": {
            "foreground": "{{--vscodet-face-strong}}"
        }
    }
}
