    "tokenColors": [
        // Core structural elements - strong face (bold emphasis)
        {
            "scope": [
                "entity.name.function",
                "entity.name.class",
                "entity.name.type",
                "entity.name.namespace",
                "entity.name.tag",
                "meta.function-call.generic.python",
                "support.function",
                "keyword.control",
                "keyword.operator.new",
                "storage.type.function",
                "storage.type.class",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#ffffff",
                "fontStyle": "bold"
            }
        },

        // Important constructs - salient face (accent color)
        {
            "scope": [
                "entity.name.type.class",
                "entity.name.type.interface",
                "entity.name.type.enum",
                "support.type",
                "support.class",
                "variable.language.this",
                "variable.language.super",
                "constant.language",
                "meta.import",
                "meta.export"
            ],
            "settings": {
                "foreground": "#dcd3f8"
            }
        },

        // Attention-grabbing elements - popout face
        {
            "scope": [
                "string",
                "string.quoted",
                "string.template",
                "constant.character.escape",
                "markup.quote",
                "markup.bold",
                "entity.name.tag.yaml"
            ],
            "settings": {
                "foreground": "#ede8fc"
            }
        },

        // Critical elements - critical face (errors, warnings, deletions)
        {
            "scope": [
                "invalid",
                "invalid.illegal",
                "invalid.deprecated",
                "markup.deleted",
                "markup.error",
                "diff.removed",
                "diff.deleted",
                "git.deleted",
                "error",
                "diagnostic.error"
            ],
            "settings": {
                "foreground": "#f38ba8"
            }
        },

        // Added/inserted elements - use green semantic color
        {
            "scope": [
                "markup.inserted",
                "markup.inserted.diff",
                "diff.inserted",
                "diff.added",
                "git.inserted",
                "git.added",
                "meta.diff.header.to-file",
                "meta.diff.range.added",
                "punctuation.definition.inserted",
                "token.info-token"
            ],
            "settings": {
                "foreground": "#a6e3a1"
            }
        },

        // Modified elements - use accent/salient color
        {
            "scope": [
                "markup.changed",
                "markup.changed.diff",
                "diff.changed",
                "diff.modified",
                "git.modified",
                "meta.diff.range.changed",
                "punctuation.definition.changed"
            ],
            "settings": {
                "foreground": "#dcd3f8"
            }
        },

        // Secondary information - faded face (comments, less important)
        {
            "scope": [
                "comment",
                "comment.line",
                "comment.block",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#808080",
                "fontStyle": "italic"
            }
        },

        // Non-comment faded elements (no italic)
        {
            "scope": [
                "markup.quote",
                "meta.preprocessor",
                "entity.other.attribute-name.html",
                "entity.other.attribute-name.class.css",
                "punctuation.separator",
                "punctuation.terminator",
                "punctuation.accessor"
            ],
            "settings": {
                "foreground": "#808080"
            }
        },

        // Default elements - default face (variables, identifiers)
        {
            "scope": [
                "variable",
                "variable.other",
                "variable.parameter",
                "variable.object.property",
                "meta.object-literal.key",
                "entity.name.variable",
                "constant.numeric",
                "constant.language.boolean",
                "support.variable"
            ],
            "settings": {
                "foreground": "#ffffff"
            }
        },

        // Operators and punctuation - default with bold
        {
            "scope": [
                "keyword.operator",
                "keyword.operator.arithmetic",
                "keyword.operator.comparison",
                "keyword.operator.logical",
                "keyword.operator.assignment",
                "punctuation.definition.string.begin",
                "punctuation.definition.string.end"
            ],
            "settings": {
                "foreground": "#ffffff",
                "fontStyle": "bold"
            }
        },

        // Language keywords - salient with bold
        {
            "scope": [
                "keyword",
                "keyword.control.flow",
                "keyword.control.conditional",
                "keyword.control.loop",
                "keyword.control.import",
                "keyword.control.export",
                "storage.type",
                "storage.modifier.async",
                "storage.modifier.static"
            ],
            "settings": {
                "foreground": "#dcd3f8",
                "fontStyle": "bold"
            }
        },

        // Types and built-ins - salient face
        {
            "scope": [
                "entity.name.type",
                "support.type.primitive",
                "support.type.builtin",
                "support.class.builtin",
                "entity.other.inherited-class"
            ],
            "settings": {
                "foreground": "#dcd3f8"
            }
        },

        // Decorators and annotations - popout face
        {
            "scope": [
                "entity.name.function.decorator",
                "meta.decorator",
                "punctuation.decorator"
            ],
            "settings": {
                "foreground": "#ede8fc"
            }
        },

        // Markup headings - strong face
        {
            "scope": [
                "markup.heading",
                "entity.name.section"
            ],
            "settings": {
                "foreground": "#ffffff",
                "fontStyle": "bold"
            }
        },

        // Links and references - salient face
        {
            "scope": [
                "markup.underline.link",
                "string.other.link"
            ],
            "settings": {
                "foreground": "#dcd3f8"
            }
        },

        // Code blocks and literal content - default face with subtle background
        {
            "scope": [
                "markup.fenced_code",
                "markup.inline.raw"
            ],
            "settings": {
                "foreground": "#ffffff",
                "background": "#232323"
            }
        }
    ],

    "semanticHighlighting": true,

    "semanticTokenColors": {
        // Core structural elements - strong face (bold for importance)
        "namespace": {
            "foreground": "#ffffff",
            "fontStyle": "bold"
        },
        "class": {
            "foreground": "#ffffff",
            "fontStyle": "bold"
        },
        "function": {
            "foreground": "#ffffff",
            "fontStyle": "bold"
        },
        "method": {
            "foreground": "#ffffff"
        },

        // Important type information - salient face (accent color)
        "interface": {
            "foreground": "#dcd3f8",
            "fontStyle": "bold"
        },
        "struct": {
            "foreground": "#dcd3f8",
            "fontStyle": "bold"
        },
        "enum": {
            "foreground": "#dcd3f8",
            "fontStyle": "bold"
        },
        "type": {
            "foreground": "#dcd3f8"
        },
        "typeParameter": {
            "foreground": "#dcd3f8"
        },

        // Language constructs - salient with bold
        "keyword": {
            "foreground": "#dcd3f8",
            "fontStyle": "bold"
        },
        "macro": {
            "foreground": "#dcd3f8",
            "fontStyle": "bold"
        },

        // Regular variables and data - default face
        "variable": {
            "foreground": "#ffffff"
        },
        "parameter": {
            "foreground": "#ffffff"
        },
        "property": {
            "foreground": "#ffffff"
        },
        "enumMember": {
            "foreground": "#dcd3f8"
        },

        // Operators - default with bold
        "operator": {
            "foreground": "#ffffff",
            "fontStyle": "bold"
        },

        // Literals and content - popout for strings, default for numbers
        "string": {
            "foreground": "#ede8fc"
        },
        "number": {
            "foreground": "#ffffff"
        },
        "regexp": {
            "foreground": "#ede8fc"
        },

        // Meta constructs - appropriate face based on function
        "decorator": {
            "foreground": "#ede8fc"
        },
        "event": {
            "foreground": "#dcd3f8"
        },
        "label": {
            "foreground": "#ffffff",
            "fontStyle": "bold"
        },
        "comment": {
            "foreground": "#808080",
            "fontStyle": "italic"
        },

        // Behavioral modifiers - emphasize declarations and important states
        "*.declaration": {
            "fontStyle": "bold"
        },
        "*.definition": {
            "fontStyle": "bold"
        },
        "*.readonly": {
            "foreground": "#dcd3f8"
        },
        "*.deprecated": {
            "foreground": "#808080",
            "fontStyle": "strikethrough"
        },
        "*.modification": {
            "fontStyle": "bold"
        },
        "*.documentation": {
            "foreground": "#808080"
        },
        "*.defaultLibrary": {
            "foreground": "#dcd3f8"
        },

        // Specific important combinations
        "class.declaration": {
            "foreground": "#ffffff",
            "fontStyle": "bold"
        },
        "function.declaration": {
            "foreground": "#ffffff",
            "fontStyle": "bold"
        },
        "method.static": {
            "foreground": "#ffffff"
        },
        "function.static": {
            "foreground": "#ffffff",
            "fontStyle": "bold"
        },
        "variable.readonly": {
            "foreground": "#dcd3f8"
        },
        "property.readonly": {
            "foreground": "#dcd3f8"
        },
        "parameter.readonly": {
            "foreground": "#dcd3f8"
        },
        "class.abstract": {
            "foreground": "#dcd3f8",
            "fontStyle": "bold"
        },
        "method.abstract": {
            "foreground": "#ffffff"
        },
        "function.async": {
            "foreground": "#ffffff",
            "fontStyle": "bold"
        },
        "method.async": {
            "foreground": "#ffffff"
        }
    }
}
